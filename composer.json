{
    "require-dev": {
        "codeception/lib-asserts": "^2.0",
        "codeception/codeception": "^5.0.0",
        "codeception/module-asserts": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
            "Turbine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TurbineTest\\": "tests/_support/",
            "TurbineTest\\Unit\\": "tests/unit/"
        }
    },
    "scripts": {
        "codecept": "php vendor/bin/codecept",
        "test": "php vendor/bin/codecept run",
        "build": "php vendor/bin/codecept build"
    }
}
